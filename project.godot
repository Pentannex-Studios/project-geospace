config_version=4

_global_script_classes=[ {
"base": "CollisionPolygon2D",
"class": "DonutCollisionPolygon2D",
"language": "GDScript",
"path": "res://addons/donut_collision_polygon2D/donut_collision_polygon2D.gd"
} ]
_global_script_class_icons={
"DonutCollisionPolygon2D": ""
}

[application]

config/name="GeoSpace"
config/description="There's an outside the world organization in the Universe called Intergalactic Aeronotics and Space Federation. It consist of 20 galaxy members. Their purpose is to make a peaceful universe on each galaxies. One of the spacescout, a guard of the IASF, discovered a new dimension. The higher ranking member of the IASF gives it a name called GeoSpace. They warned everyone not to go there because it might be a too dangerous place. Also they still have no proper tool to comeback from the GeoSpace to the original Universe. Unfortunately, one of the spacescout didn't heard about the warning and they went to the mysterious ModuSpace without a plan on going back. They managed to get in there but their teleporter got destroyed from the pressure in space while travelling. The spacescout had to explore to learn how to survive and to get out in... GeoSpace."
run/main_scene="res://scn/menu/menu.tscn"
run/delta_sync_after_draw=true
boot_splash/bg_color=Color( 0, 0, 0, 1 )

[autoload]

lib="*res://src/sameCode.gd"
sfx="*res://obj/sound/player.tscn"

[display]

window/size/resizable=false
window/size/borderless=true

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/donut_collision_polygon2D/plugin.cfg" )

